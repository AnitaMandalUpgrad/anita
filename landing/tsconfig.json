{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "strictTemplates": true
  },
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "outDir": "./anita-app.github.io/out-tsc",
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@anita/landing/*": [
        "src/app/*"
      ]
    }
  }
}