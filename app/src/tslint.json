{
  "extends": "../tslint.json",
  "cliOptions": {
    "exclude": [
      "assets/js"
    ]
  },
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}